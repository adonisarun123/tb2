{
  "name": "apple-gradient-hero",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5173",
    "build": "tsc && vite build && npm run generate-sitemap",
    "build-only": "tsc && vite build",
    "build-prod": "NODE_ENV=production tsc && vite build --mode production && npm run generate-sitemap",
    "build-with-analysis": "tsc && vite build && npm run generate-sitemap && npm run analyze-bundle",
    "generate-sitemap": "tsx scripts/generateSitemap.ts",
    "embed-activities": "tsx scripts/embedActivities.ts",
    "create-pinecone-index": "tsx scripts/createPineconeIndex.ts",
    "update-activity-content": "tsx scripts/updateActivityContent.ts",
    "preview": "vite preview",
    "clean": "rimraf dist node_modules",
    "start": "npm run build && vite preview",
    "analyze-bundle": "npx vite-bundle-analyzer dist/stats.html",
    "serve": "node server.ts",
    "test-performance": "npm run build && npm run serve",
    "audit": "npm audit && npm run build && bundlesize"
  },
  "bundlesize": [
    {
      "path": "./dist/assets/js/*.js",
      "maxSize": "250 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/assets/css/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ],
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^2.2.0",
    "@pinecone-database/pinecone": "^6.1.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.17",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.4",
    "express": "^4.21.2",
    "framer-motion": "^10.18.0",
    "openai": "^5.8.1",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.15.1",
    "react-router-dom": "^7.1.5",
    "tailwindcss": "^3.4.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.2.1",
    "bundlesize": "^0.18.2",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "dotenv": "^17.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rimraf": "^5.0.5",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vite": "^6.2.1",
    "vite-bundle-analyzer": "^0.11.1"
  }
}
